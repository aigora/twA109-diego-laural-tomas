#include <stdio.h>
#include <stdlib.h>


#define identificador

// material:   1=metal      2=madera        3=papel



typedef struct 
{
	float constante;
} entrada;

int indentificador (entrada ent);
void imprimir (material m);
void ejecutar (material m);

int main (void)
{int material;

entrada ent;
material= identificador(ent);
imprimir(material);
ejecutar(material);

system ("PAUSE");
return 0;	
}


int indentificador (entrada ent)
{ int material=0;
	scanf("%f", &constante.ent);
	
    if (ent>=0 && ent<2)
    { material=1;}
    
    if (ent>=2 && ent<4)
    { material=2;}
    
    if (ent>=4 && ent<6)
    { material=3;}

    else
     printf("Material no identificado");
     
     return material;
}

void imprimir (material m)
{   if (m=1)
	printf("El material es METAL");
	
	if (m=2)
	printf("El material es MADERA");
	
	if (m=3)
	printf("El material es PAPEL");	
}

void ejecutar (material m)
{
switch(m)
{
       case 1: fputc(A);    //A=encender motor1
             
             break;
             
       case 2:  fputc(B);    //B=encender motor2
             
             break;
             
        case 3:  fputc(C);    //C=encender motor3
             break;
}
}
